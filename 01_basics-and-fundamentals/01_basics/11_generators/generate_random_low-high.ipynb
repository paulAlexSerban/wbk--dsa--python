   "source": [
    "import random\n",
    "\n",
    "def random_numbers(low, high, n):\n",
    "    for i in range(n):\n",
    "        yield random.randint(low, high)\n",
    "        \n",
    "for num in random_numbers(1, 10, 12):\n",
    "    print(num)"
   ]
